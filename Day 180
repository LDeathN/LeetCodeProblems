#First Problem (468)
class Solution(object):
    def validIPAddress(self, queryIP):
        def isIPv4(s):
            parts = s.split(".")
            if len(parts) != 4:
                return False
            for part in parts:
                if not part.isdigit() or not 0 <= int(part) <= 255:
                    return False
                if part[0] == '0' and len(part) > 1:  
                    return False
            return True

        def isIPv6(s):
            parts = s.split(":")
            if len(parts) != 8:
                return False
            hex_digits = "0123456789abcdefABCDEF"
            for part in parts:
                if not (1 <= len(part) <= 4):
                    return False
                if not all(c in hex_digits for c in part):
                    return False
            return True

        if isIPv4(queryIP):
            return "IPv4"
   
        if isIPv6(queryIP):
            return "IPv6"

        return "Neither"



#Second Problem (470)
import random
class Solution(object):
    def rand10(self):
        while True:
            num = (rand7() - 1) * 7 + rand7()
            if num <= 40:
                return (num % 10) + 1





