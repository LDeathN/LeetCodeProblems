#First Problem (1049)
class Solution(object):
    def lastStoneWeightII(self, stones):
        total_sum = sum(stones)
        target = total_sum // 2
        dp = [False] * (target + 1)
        dp[0] = True  
        
        for stone in stones:
            for j in range(target, stone - 1, -1):
                dp[j] = dp[j] or dp[j - stone]
        
        for sum1 in range(target, -1, -1):
            if dp[sum1]:
                return total_sum - 2 * sum1




