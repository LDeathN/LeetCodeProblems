#First Problem 1227
class Solution(object):
    def nthPersonGetsNthSeat(self, n):
        if n == 1:
            return 1.0
        else:
            return 0.5



#Second Problem 1233
class Solution(object):
    def removeSubfolders(self, folder):
        folder.sort()
        result = []
        
        for f in folder:
            if not result or not f.startswith(result[-1] + '/'):
                result.append(f)
        
        return result




