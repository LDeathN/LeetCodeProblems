#First Problem (477)
class Solution(object):
    def totalHammingDistance(self, nums):
        total_distance = 0
        n = len(nums)

        for bit in range(32):
            count_ones = 0

            for num in nums:
                if (num >> bit) & 1:
                    count_ones += 1

            count_zeros = n - count_ones

            total_distance += count_ones * count_zeros
        
        return total_distance




