#First Problem 1261
class FindElements(object):

    def __init__(self, root):
        self.recovered_values = set()

        def recover(node, val):
            if not node:
                return
            node.val = val
            self.recovered_values.add(val)
            recover(node.left, 2 * val + 1)
            recover(node.right, 2 * val + 2)

        recover(root, 0)
        

    def find(self, target):
        return target in self.recovered_values



#Second Problem 1262
class Solution(object):
    def maxSumDivThree(self, nums):
        mod1 = []
        mod2 = []
        total_sum = 0

        for num in nums:
            total_sum += num
            if num % 3 == 1:
                mod1.append(num)
            elif num % 3 == 2:
                mod2.append(num)
        
        mod1.sort()
        mod2.sort()

        remainder = total_sum % 3
        if remainder == 0:
            return total_sum
        elif remainder == 1:
            remove_one = mod1[0] if mod1 else float('inf')
            remove_two = sum(mod2[:2]) if len(mod2) >= 2 else float('inf')
            return total_sum - min(remove_one, remove_two)
        elif remainder == 2:
            remove_one = mod2[0] if mod2 else float('inf')
            remove_two = sum(mod1[:2]) if len(mod1) >= 2 else float('inf')
            return total_sum - min(remove_one, remove_two)



#Third Problem 1267
class Solution(object):
    def countServers(self, grid):
        m, n = len(grid), len(grid[0])
    
        row_counts = [0] * m
        col_counts = [0] * n
        
        for i in range(m):
            for j in range(n):
                if grid[i][j] == 1:
                    row_counts[i] += 1
                    col_counts[j] += 1
        
        count = 0
        for i in range(m):
            for j in range(n):
                if grid[i][j] == 1 and (row_counts[i] > 1 or col_counts[j] > 1):
                    count += 1
                    
        return count



#Fourth Problem 1268
class Solution(object):
    def suggestedProducts(self, products, searchWord):
        products.sort()
    
        result = []
        prefix = ""
        
        for char in searchWord:
            prefix += char
            
            matches = [product for product in products if product.startswith(prefix)]
            
            result.append(matches[:3])
        
        return result




