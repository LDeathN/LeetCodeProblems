#First Problem (740)
class Solution(object):
    def deleteAndEarn(self, nums):
        points = Counter(nums)
        max_num = max(nums)
        
        dp = [0] * (max_num + 1)
        
        dp[1] = points[1] * 1  
        
        for x in range(2, max_num + 1):
            dp[x] = max(dp[x - 1], dp[x - 2] + points[x] * x)
        
        return dp[max_num]




