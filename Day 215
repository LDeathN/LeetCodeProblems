#First Problem (945)
class Solution(object):
    def minIncrementForUnique(self, nums):
        nums.sort() 
        moves = 0

        for i in range(1, len(nums)):
            if nums[i] <= nums[i - 1]:  
                increment = nums[i - 1] + 1 - nums[i] 
                nums[i] = nums[i - 1] + 1
                moves += increment  
        
        return moves



#Second Problem (946)
class Solution(object):
    def validateStackSequences(self, pushed, popped):
        stack = []
        j = 0 

        for x in pushed:
            stack.append(x)  
            while stack and stack[-1] == popped[j]:
                stack.pop()
                j += 1
        
        return not stack




