#First Problem (528)
class Solution(object):

    def __init__(self, w):
        self.prefix_sum = []
        current_sum = 0
        
        for weight in w:
            current_sum += weight
            self.prefix_sum.append(current_sum)
        
        self.total_sum = current_sum
        

    def pickIndex(self):
        target = random.randint(1, self.total_sum)

        low, high = 0, len(self.prefix_sum) - 1
        while low < high:
            mid = (low + high) // 2
            if self.prefix_sum[mid] < target:
                low = mid + 1
            else:
                high = mid
        
        return low




